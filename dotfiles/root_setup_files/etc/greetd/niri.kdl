output "DP-2" {
  mode "1920x1080@60.000"
  transform "90"
  position x=0 y=0
  off
}

// Center Monitor
output "DP-1" {
  mode "2560x1440@144.000"
  position x=1080 y=0
  focus-at-startup
}

// Right Monitor
output "HDMI-A-1" {
  mode "1920x1080@60.000"
  transform "270"
  position x=3640 y=0
  off
}

hotkey-overlay {
    skip-at-startup
}

environment {
    DMS_RUN_GREETER "1"
}

gestures {
  hot-corners {
    off
  }
}
